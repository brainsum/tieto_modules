{
    "name": "brainsum/tieto_modules",
    "description": "Umbrella for Tieto modules.",
    "type": "drupal-module",
    "license": "proprietary",
    "authors": [
        {
            "name": "mhavelant",
            "email": "mhavelant@brainsum.com"
        },
        {
            "name": "mdanko",
            "email": "mdanko@brainsum.com"
        }
    ],
    "repositories": {
        "drupal": {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    },
    "require": {
        "php": "^7.2",
        "roave/security-advisories": "dev-master",
        "drupal/core": "^8.5",
        "cweagans/composer-patches": "~1.5",
        "wikimedia/composer-merge-plugin": "^1.4",
        "brainsum/tieto_lifecycle_management": "^1.0"
    },
    "require-dev": {
        "brainsum/drupal-behat-testing": "^0.11"
    },
    "replace": {
        "brainsum/elevation_wysiwyg": "*",
        "brainsum/tieto_tibr": "*",
        "tieto/tieto_linkit": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "enable-patching": true,
        "merge-plugin": {
            "include": [
                "*/composer.json"
            ],
            "recurse": true,
            "replace": false,
            "ignore-duplicates": false,
            "merge-dev": true,
            "merge-extra": true,
            "merge-extra-deep": false,
            "merge-scripts": false
        }
    },
    "autoload": {
        "psr-4": {
            "Brainsum\\TietoModules\\tieto_lifecycle_management\\Tests\\Behat\\": "tieto_lifecycle_management/tests/Behat/"
        }
    }
}
